{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "lib": ["es2018"],
    "baseUrl": ".",
    "outDir": "./dist",
    "strict": false,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "plugins": [{
        "name": "scoped-typescript-plugin",
        "paths": [
          "./src"
        ]
    }]
  }
}
